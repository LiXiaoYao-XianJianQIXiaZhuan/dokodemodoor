/* pages/personal/help/problem/problem.wxss */
page{
    width: 100%;
    height: 100%;
    background-color: white;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.problem{
    width: 90%;
    font-size: 52rpx;
    font-weight: bold;
    margin: 20rpx 20rpx 20rpx 20rpx;
    color: black;
}
.content{
    width: 90%;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 0 20rpx 20rpx 20rpx;
}
#c1,#c2{
    width: 96%;
    font-size: 36rpx;
    color: #9CA3AF;
}
.feedback{
    width: 90%;
    position: absolute;
    bottom: 10rpx;
    border-radius: 100rpx;
    color: #73CFFF;
}